{
  "name": "@bufferapp/components",
  "version": "0.5.22",
  "description": "A shared set of UI Components",
  "main": "index.js",
  "scripts": {
    "start": "start-storybook -p 9001",
    "lint": "./node_modules/eslint/bin/eslint.js . --ignore-pattern coverage .storybook node_modules",
    "test": "yarn run lint && sh ../../package_test.sh",
    "test-watch": "jest --watch",
    "test-update": "jest -u"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bufferapp/buffer-components.git"
  },
  "author": "Harrison Harnisch <hharnisc@gmail.com> (https://hharnisc.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bufferapp/buffer-components/issues"
  },
  "homepage": "https://github.com/bufferapp/buffer-components#readme",
  "peerDependencies": {
    "react": ">= 0.14.0"
  },
  "devDependencies": {
    "@storybook/addon-storyshots": "3.1.4",
    "@storybook/react": "3.1.3",
    "eslint": "3.19.0",
    "jest": "19.0.2",
    "storybook-addon-a11y": "3.0.0"
  },
  "dependencies": {
    "react-autocomplete": "1.5.9",
    "react-day-picker": "5.5.1"
  }
}
